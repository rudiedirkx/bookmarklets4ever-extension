{
	"name": "Bookmarklets 4ever",
	"version": "1.3",
	"manifest_version": 3,

	"description": "Makes bookmarklets work again, by removing all CSP headers =(",
	"icons": {
		"128": "logo_128.png"
	},

	"permissions": [
		"declarativeNetRequestWithHostAccess"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"declarative_net_request" : {
		"rule_resources" : [
			{
				"id": "ruleset_1",
				"enabled": true,
				"path": "net_rules.json"
			}
		]
	}
}
